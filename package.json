{
	"name": "demo",
	"version": "0.1.0",
	"bin": {
		"sample": "dist/app.js"
	},
	"scripts": {
		"lint": "./node_modules/.bin/eslint ./src --ext .ts",
		"format": "./node_modules/.bin/prettier . --write",
		"build": "npm run clean && tsc --skipLibCheck true",
		"clean": "rmdir-cli dist && rmdir-cli cdk.out",
		"watch": "tsc -w",
		"test": "jest",
		"cdk": "cdk",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@types/jest": "^29.4.0",
		"@types/node": "^18.14.2",
		"@typescript-eslint/eslint-plugin": "^5.54.0",
		"@typescript-eslint/parser": "^5.54.0",
		"aws-cdk-lib": "^2.81.0",
		"cdk-nag": "^2.22.19",
		"constructs": "^10.1.262",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-config-standard-with-typescript": "^34.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"husky": "^8.0.3",
		"jest": "^29.4.3",
		"lint-staged": "^13.1.2",
		"prettier": "^2.8.4",
		"rmdir-cli": "^2.0.6",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@aws-sdk/client-cloudformation": "^3.279.0",
		"@aws-sdk/client-codepipeline": "^3.279.0",
		"@aws-sdk/client-dynamodb": "^3.279.0",
		"@aws-sdk/types": "^3.272.0"
	},
	"lint-staged": {
		"*.ts": "npm run lint",
		"*.{js,css,md,html,json}": "npm run format"
	}
}
